                "chop": 1.3,
                "squeeze": 1.2,
                "trend_up": 1.2,
                "trend_down": 1.2,
                "default": 1.2
            },
            "vwap_lose": {
                "chop": 1.3,
                "squeeze": 1.2,
                "trend_up": 1.2,
                "trend_down": 1.2,
                "default": 1.2
            },
            "premium_zone": {
                "_comment": "From baseline: chop=1.3, trend=0.9 - premium/discount zones work best in ranging",
                "chop": 1.3,
                "squeeze": 1.2,
                "trend_up": 0.9,
                "trend_down": 0.9,
                "default": 1.0
            },
            "discount_zone": {
                "_comment": "From baseline: chop=1.3, trend=0.9 - premium/discount zones work best in ranging",
                "chop": 1.3,
                "squeeze": 1.2,
                "trend_up": 0.9,
                "trend_down": 0.9,
                "default": 1.0
            },
            "orb_pullback": {
                "chop": 1.1,
                "squeeze": 1.0,
                "trend_up": 1.1,
                "trend_down": 1.1,
                "default": 1.0
